{
  "name": "git-chaos-action",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start:local": "node -r dotenv/config main.js",
    "build": "ncc build -m main.js",
    "debug": "node -r dotenv/config --nolazy --inspect-brk=9229 main.js"
  },
  "author": "chaos monkeys",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@octokit/rest": "^16.35.0",
    "aws-sdk": "^2.590.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@zeit/ncc": "^0.20.5",
    "dotenv": "^8.2.0",
    "husky": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run build && git add ./dist/index.js"
    }
  }
}
